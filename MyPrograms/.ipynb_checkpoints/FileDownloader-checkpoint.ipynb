    "        except Exception as inst:\n",
    "            print(inst)\n",
    "            print('  Encountered unknown error. Continuing.')\n",
    "    \n",
    "    #File exists; don't download\n",
    "    else:\n",
    "        print(\"This file exists already.\")\n",
    "\n",
    "#End of program\n",
    "print(\"Finished downloading.\")"
